{
  "name": "peachtree-bank-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-en": "ng serve --configuration en",
    "start-pl": "ng serve --configuration pl",
    "build": "ng build",
    "build-localize": "ng build --localize",
    "build-en": "ng build --configuration en",
    "build-pl": "ng build --configuration pl",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "npm run _i18n && npm run _extract-i18n-from-code",
    "combine-i18n": "npm run _merge-xlf && npm run _xliffmerge",
    "_i18n": "ng xi18n peachtree-bank-test --format xlf2 --output-path src/i18n --progress",
    "_extract-i18n-from-code": "node scripts/extract-localization-from-code.js",
    "_merge-xlf": "node scripts/merge-xlf.js src/i18n/custom.xlf --main src/i18n/messages.xlf --output src/i18n/messages.xlf",
    "_xliffmerge": "ng run peachtree-bank-test:xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/cdk": "^10.2.2",
